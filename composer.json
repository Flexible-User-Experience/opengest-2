{
    "name": "flux/opengest-2",
    "description": "Custom Symfony 2 webapp ERP to manage proprietary business and website",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "beberlei/doctrineextensions": "^1.0",
        "beelab/recaptcha2-bundle": "^1.3",
        "components/font-awesome": "^4.0",
        "components/jquery": "^2.0",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "doctrine/migrations": "^1.0",
        "doctrine/orm": "^2.4.8",
        "egeloen/ckeditor-bundle": "^4.0",
        "egulias/email-validator": "^1.2",
        "friendsofsymfony/jsrouting-bundle": "^2.0",
        "hampe/zurb-ink-bundle": "^2.2",
        "hautelook/alice-bundle": "^1.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "ircmaxell/password-compat": "~1.0",
        "knplabs/knp-menu-bundle": "^2.0",
        "knplabs/knp-paginator-bundle": "^2.5",
        "liip/functional-test-bundle": "^1.0",
        "liip/imagine-bundle": "^1.0",
        "php": ">=7.2.0",
        "presta/sitemap-bundle": "^1.3",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sonata-project/admin-bundle": "^3.0",
        "sonata-project/core-bundle": "3.16.1",
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "sonata-project/user-bundle": "^3.0",
        "symfony/assetic-bundle": "^2.7",
        "symfony/monolog-bundle": "^3.0.2",
        "symfony/symfony": "2.8.*",
        "symfony/swiftmailer-bundle": "~2.3",
        "stof/doctrine-extensions-bundle": "^1.2",
        "tecnickcom/tcpdf": "^6.0",
        "twbs/bootstrap": "^3.0",
        "twig/twig": "2.10.0",
        "vich/uploader-bundle": "^1.1",
        "xsolve-pl/xsolve-cookie-acknowledgement-bundle": "^1.0",
        "composer/package-versions-deprecated": "1.11.99.1"
    },
    "require-dev": {
        "cypresslab/gitelephant": "3.0.0",
        "cypresslab/gitelephant-bundle": "^0.0.6",
        "sensio/generator-bundle": "~3.0",
        "phpunit/phpunit": "^5.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
